<?xml version="1.0" encoding="UTF-8"?>
<databaseChangeLog 
  xmlns="http://www.liquibase.org/xml/ns/dbchangelog"
  xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns:ext="http://www.liquibase.org/xml/ns/dbchangelog-ext"
  xsi:schemaLocation="http://www.liquibase.org/xml/ns/dbchangelog http://www.liquibase.org/xml/ns/dbchangelog/dbchangelog-3.1.xsd
        http://www.liquibase.org/xml/ns/dbchangelog-ext http://www.liquibase.org/xml/ns/dbchangelog/dbchangelog-ext.xsd">

  <changeSet author="mdnasim" id="Container type id column for os storage container audit table">
    <addColumn tableName="os_storage_containers_aud">
      <column name="CONTAINER_TYPE_ID" type="${int.type}"/>
    </addColumn>
  </changeSet>

  <changeSet author="mdnasim" id="Container type id column for os storage container audit table modified">
    <addColumn tableName="os_storage_containers_aud">
      <column name="containerType_MOD" type="${bit.type}"/>
    </addColumn>
  </changeSet>
 </databaseChangeLog>